{
  "name": "turborepo-pnpm",
  "private": true,
  "scripts": {
    "build": "turbo build --cache-dir=.turbo",
    "dev": "turbo dev",
    "lint": "turbo lint --cache-dir=.turbo",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "prettier": "^3.2.5",
    "turbo": "^1.13.3"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  }
}
